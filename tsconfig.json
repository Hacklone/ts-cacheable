{
  "compilerOptions": {
    "sourceMap": true,
    "target": "es5",
    "module": "commonjs",
    "declaration": true,
    "outDir": "./dist",
    "experimentalDecorators": true,
    "lib": ["es2016", "dom"],
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "baseUrl": "./",
    "paths": {
      "ngx-cacheable/common/*": ["./dist/common/*"]
    }
  },
  "include": ["**/*.ts"]
}
